{
  "expo": {
    "name": "ColetaSC",
    "slug": "ColetaSC",
    "version": "1.0.0",
    "platforms": [
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#27AE60"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "br.edu.ifsc.coletasc" 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#27AE60"
      },
      "package": "br.edu.ifsc.coletasc"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
